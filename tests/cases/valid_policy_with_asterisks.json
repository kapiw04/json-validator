{
    "PolicyName": "ValidMultipleStatements",
    "PolicyDocument": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Sid": "Stmt1",
          "Effect": "Allow",
          "Action": ["s3:ListBucket"],
          "Resource": "arn:aws:s3:::example_bucket/*"
        },
        {
          "Sid": "Stmt2",
          "Effect": "Allow",
          "Action": ["s3:GetObject"],
          "Resource": "arn:aws:s3:::example_bucket/all/*"
        }
      ]
    }
  }